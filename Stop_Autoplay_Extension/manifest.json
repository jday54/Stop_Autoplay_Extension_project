{
  "manifest_version": 3,
  "name": "Stop Youtube Autoplay",
  "description": "Stop Youtube Autoplay feature for Watch Later videos",
  "version": "1.0",
  "permissions": [
    "webNavigation"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "icons": {
    "128": "images/icon.png"
  },
  "action": {
    "default_title": "Stop Youtube Autoplay",
    "default_popup": "popup.html",
    "default_icon": {
        "128": "images/icon.png"
    }
  }
}
